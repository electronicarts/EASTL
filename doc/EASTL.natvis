        <LinkedListItems>
            <HeadPointer>mAnchor.mpNext</HeadPointer>
            <NextPointer>mpNext</NextPointer>
            <ValueNode>*this</ValueNode>
        </LinkedListItems>
    </Expand>
</Type>

<Type Name="eastl::intrusive_list_iterator&lt;*&gt;">
